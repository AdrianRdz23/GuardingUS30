@model List<GuardingUS30.Models.UserHomeModel>


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Home Catalog</title>
</head>
<body>
    <h4>Homes</h4>
    <hr />
    <table cellpadding="0" cellspacing="0" style="width:70%">
        <tr>
            <th>IDHome</th>
            <th>Owner</th>
            <th>NUMBER</th>
            <th>CARS</th>
            <th>ADDRESS</th>
            

        </tr>

        @foreach (UserHomeModel home in Model)
        {
            <tr>
                <td>@home.homeModel.idhome</td>
                <td>@home.userModel.name</td>
                <td>@home.homeModel.number</td>
                <td>@home.homeModel.cars</td>
                <td>@home.homeModel.address</td>
                <td><a class="nav-link text-dark" asp-area="" asp-controller="Admin" asp-action="EditHomeView" asp-route-id="@home.homeModel.idhome">Edit</a></td>
                <td><a class="nav-link text-dark" asp-area="" asp-controller="Admin" asp-action="DeleteHome" asp-route-id="@home.homeModel.idhome">Delete</a></td>
            </tr>
        }

    </table>

    <a class="nav-link text-dark" asp-area="" asp-controller="Login" asp-action="AddHomeView">Add Home</a>

</body>
</html>