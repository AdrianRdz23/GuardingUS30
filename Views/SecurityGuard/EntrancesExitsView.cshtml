@model List<GuardingUS30.Models.VisitorsHomeModel>


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Visitors</title>
</head>
<body>
    <h4>Visitors</h4>
    <hr />
    <table cellpadding="0" cellspacing="0" style="width:100%">
        <tr>
            <th>NAME</th>
            <th>CAR PLATE</th>
            <th>ADDRESS</th>
            <th>IDENTIFICATION</th>
            <th>ENTRANCE</th>
            <th>EXIT</th>
        </tr>

        @foreach (VisitorsHomeModel visitor in Model)
        {
    <tr>
        <td>@visitor.visitors.name</td>
        <td>@visitor.visitors.carPlate </td>
        <td>@visitor.home.address</td>
        <td>@visitor.visitors.identification</td>
        <td>@visitor.visitors.entrance</td>
        <td>@visitor.visitors.exit</td>
        <td><a class="nav-link text-dark" asp-area="" asp-controller="SecurityGuard" asp-action="UpdateVisitor" asp-route-id="@visitor.visitors.idvisitor">Update Exit</a></td>
    </tr>
        }

    </table>



</body>
</html>